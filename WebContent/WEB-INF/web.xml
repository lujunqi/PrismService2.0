<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>WeiXinQYAction</servlet-name>
		<servlet-class>com.weixin.WeiXinQYAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WeiXinQYAction</servlet-name>
		<url-pattern>/qweixin</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PrismAction2</servlet-name>
		<servlet-class>com.prism.action.PrismActionEx</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PrismAction2</servlet-name>
		<url-pattern>/pa/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PrismAction</servlet-name>
		<servlet-class>com.prism.action.PrismAction</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PrismAction</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>basePath</param-name>
		<param-value>e:/temp/</param-value>
	</context-param>



</web-app>